<tr class="text-center">
  <td><%= category.id %></td>
  <td><%= link_to category.name, "#" %></td>
  <td><%= category.created_at %></td>
  <td class="control-custom">
    <span><%= link_to "", edit_partners_category_path(category),
                              class: "btn btn-success glyphicon glyphicon-edit" %></span>
    <span><%= link_to "", partners_category_path(category),
                                data: { confirm: t('admin.control.confirm_delete') },
                                method: :delete, class: "btn btn-danger glyphicon glyphicon-trash" %></span>
  </td>
</tr>
